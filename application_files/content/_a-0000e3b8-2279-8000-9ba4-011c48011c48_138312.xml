    <versionUuid>_a-0000e3b8-2279-8000-9ba4-011c48011c48_138866</versionUuid>
    <rule>
        <name>BAM_IsMobileNumberValid</name>
        <uuid>_a-0000e3b8-2279-8000-9ba4-011c48011c48_138312</uuid>
        <description>To check validity of mobile number</description>
        <parentUuid>_a-0000e3b8-2279-8000-9ba4-011c48011c48_135254</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(fn!isnull(ri!mobile),ri!mobile=""),
  true(),
  length(ri!mobile) = 0
)</definition>
        <namedTypedValue>
            <name>mobile</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
