    <rule>
        <name>BAM_IsVehicleDenied</name>
        <uuid>_a-0000e3b8-2279-8000-9ba4-011c48011c48_136327</uuid>
        <description>If either the counter input is greater than the Approval Counter or the supervisor chose denied, then this rule returns true.</description>
        <parentUuid>_a-0000e3b8-2279-8000-9ba4-011c48011c48_135254</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if( 
  or(
    ri!counter &gt; #"_a-0000e3b8-2279-8000-9ba4-011c48011c48_136322",
    ri!decision = #"_a-0000e3b8-2279-8000-9ba4-011c48011c48_135249"[1] ),
    true, 
    false )</definition>
        <namedTypedValue>
            <name>counter</name>
            <type>
